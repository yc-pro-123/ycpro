{ 
  "version":2,
  "builds":[ {
    "src":"app.py",
    "use":"@vercel/python" }
  ]
  ,
  "routes":[ {
  "src":"/(.*)",
  "dest":"app.py" }
  ]
  ,
  "headers":[{
      "source": "/(.*)",
      "headers": [
        {
          "key": "Host",
          "value": None
        }
      ]
  }  
  ]
  ,
  "build": {
    "env": {
      "VERCEL_DISABLE_AUTO_HEADERS": "1"
    }
  }


}  
  
  
